@model InventoryManager.ViewModel.CustomerData

    <div class="row">
        @Html.LabelFor(h => h.Name, htmlAttributes: new { @class = "col-sm-2" })
        <div class="col-sm-10">
            <div class="form-group">
                <div class="data-box">@Model.Name</div>
            </div>
        </div>
    </div>
    <div class="row">
        @Html.LabelFor(h => h.Description, htmlAttributes: new { @class = "col-sm-2" })
        <div class="col-sm-10">
            <div class="form-group">
                <div class="data-box">@Model.Description</div>
            </div>
        </div>
    </div>
